<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>sale_with_discount__c</fullName>
    <description>Valor de venda com desconto aplicado.</description>
    <formula>sale_value__c - (sale_value__c  *  Discount_in_cash__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Valor de venda com desconto aplicado.</inlineHelpText>
    <label>venda_com_desconto</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
